# Uncomment the next line to define a global platform for your project

 platform :ios, '8.0'

target 'AMDb' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
    use_frameworks!
    pod 'AFNetworking'
    pod 'Realm'
    pod 'Mantle'
    pod 'MBProgressHUD'

  # Pods for AMDb

  target 'AMDbTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AMDbUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
